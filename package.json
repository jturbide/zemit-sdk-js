{
  "name": "@zemit-cms/sdk-js",
  "version": "1.0.0",
  "description": "Zemit CMS - SDK",
  "main": "dist/zemit-sdk.cjs.js",
  "module": "dist/zemit-sdk.esm.js",
  "browser": "dist/zemit-sdk.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc && npm run rollup",
    "rollup": "npx rollup --config",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "Julien Turbide",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/plugin-transform-modules-commonjs": "^7.22.5",
    "@babel/preset-env": "^7.22.7",
    "@babel/preset-typescript": "^7.22.5",
    "@jest/globals": "^29.6.1",
    "@types/ip-address": "^7.0.0",
    "@types/jest": "^29.5.2",
    "@types/jsbn": "^1.2.30",
    "@types/jsdom": "^21.1.1",
    "@types/uuid": "^9.0.2",
    "babel-jest": "^29.6.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jsdom": "^22.1.0",
    "prettier": "^2.8.8",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "camel-case": "^4.1.2",
    "deepmerge": "^4.3.1",
    "ip-address": "^8.1.0",
    "jose": "^4.14.4",
    "moment": "^2.29.4",
    "nanoid": "^4.0.2",
    "store2": "^2.14.2",
    "uuid": "^9.0.0"
  },
  "files": [
    "dist/**/*"
  ]
}
